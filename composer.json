{
    "name": "douyuxingchen/php-library-stateless",
    "description": "无状态的 PHP 服务库！通过本库，您可以轻松地管理项目中的不同服务，并确保它们在各种情况下都能保持正确的状态。",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Douyuxingchen\\PhpLibraryStateless\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "libin",
            "email": "libin@douyuxingchen.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "laravel/framework": "^6.0",
        "guzzlehttp/guzzle": "^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
